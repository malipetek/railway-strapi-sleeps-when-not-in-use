  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Proxied",
      "request": "launch",
      "runtimeArgs": [
        "managed:develop:local"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Launch Normal Develop",
      "request": "launch",
      "runtimeArgs": [
        "run", "-e", "development", "-s", "Strapi", "yarn", "develop"
      ],
      "runtimeExecutable": "railway",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    }
  ]
}
